<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox styleClass="window-frame" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.mdvsc.tools.xiaomi.skin.controller.PreferenceController">
    <AnchorPane prefWidth="470.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <Separator prefHeight="0.5" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0" />
            <Label alignment="CENTER" layoutX="275.0" layoutY="20.0" prefHeight="50.0" prefWidth="570.0" style="-fx-background-color: #FFFFFF;" text="设置" textFill="#333333" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <font>
                    <Font size="16.0" />
                </font></Label>
            <Button id="closeButton" alignment="CENTER" layoutX="547.0" layoutY="4.0" onAction="#actionClose" prefHeight="40.0" prefWidth="40.0" textFill="#999999" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
                <font>
                    <Font size="15.0" />
                </font>
            </Button>
        </children>
    </AnchorPane>
    <VBox prefWidth="600.0" style="-fx-background-color: #F5F5F5;">
        <VBox.margin>
            <Insets />
        </VBox.margin>
        <padding>
            <Insets bottom="15.0" left="100.0" right="100.0" top="40.0" />
        </padding>
        <children>
         <HBox alignment="CENTER" prefHeight="21.0" prefWidth="400.0">
            <children>
               <Label alignment="CENTER" text="无线应用主题" textFill="#666666">
                  <font>
                     <Font size="15.0" />
                  </font></Label>
            </children>
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </HBox>
            <Label text="ip" textFill="#999999">
                <font>
                    <Font size="14.0" />
                </font>
                <VBox.margin>
                    <Insets left="10.0" top="4.0" />
                </VBox.margin>
            </Label>
            <TextField fx:id="textFieldIp" prefHeight="40.0" prefWidth="400.0" promptText="192.168.1.100">
                <VBox.margin>
                    <Insets top="4.0" />
                </VBox.margin>
                <font>
                    <Font size="16.0" />
                </font>
            </TextField>
            <Label text="端口" textFill="#999999">
                <VBox.margin>
                    <Insets left="10.0" top="19.0" />
                </VBox.margin>
                <font>
                    <Font size="14.0" />
                </font>
            </Label>
            <TextField fx:id="textFieldPort" prefHeight="40.0" prefWidth="400.0" promptText="5555">
                <VBox.margin>
                    <Insets top="4.0" />
                </VBox.margin>
                <font>
                    <Font size="16.0" />
                </font>
            </TextField>
            <ButtonBar>
                <buttons>
                    <Button alignment="CENTER" mnemonicParsing="false" onAction="#actionOk" prefHeight="40.0" prefWidth="80.0" text="确认" textFill="#333333">
                        <font>
                            <Font size="15.0" />
                        </font>
                    </Button>
                </buttons>
                <VBox.margin>
                    <Insets top="30.0" />
                </VBox.margin>
            </ButtonBar>
         <Separator prefWidth="200.0">
            <VBox.margin>
               <Insets top="40.0" />
            </VBox.margin>
         </Separator>
         <HBox alignment="CENTER">
            <children>
               <Label alignment="CENTER" text="关于编辑器" textFill="#666666">
                  <font>
                     <Font size="15.0" />
                  </font></Label>
            </children>
            <VBox.margin>
               <Insets top="20.0" />
            </VBox.margin>
         </HBox>
         <HBox alignment="CENTER" spacing="50.0">
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </HBox>
         <Label prefHeight="20.0" prefWidth="150.0" text="QQ群:626989602" textFill="#999999">
            <font>
               <Font size="12.0" />
            </font>
         </Label>
         <Label prefHeight="20.0" prefWidth="150.0" text="当前版本: v1.6.0" textFill="#999999">
            <font>
               <Font size="12.0" />
            </font>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </Label>
        </children>
    </VBox>
